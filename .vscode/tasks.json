{
	"version": "0.1.0",
	"command": "${workspaceRoot}/node_modules/.bin/webpack",
	"isShellCommand": true,
	"args": [
		"--progress"
	],
	"echoCommand": true,
	"tasks": [
		{
			"args": [
        "--config",
        "${workspaceRoot}/webpack.config.js"
      ],
      
			"suppressTaskName": true,
			"taskName": "webpack dev",
			"isBuildCommand": true
		},
		{
			"args": [
				"--config",
        "${workspaceRoot}/webpack.production.config.js"
			],
			"suppressTaskName": true,
			"taskName": "webpack dist",
			"isBuildCommand": false
		}
	]
}