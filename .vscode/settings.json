{
    "editor.fontSize": 14,
    "editor.fontFamily": "Fira Code, 'Fira Code', Fira Code",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "mochaExplorer.optsFile": "${workspaceFolder}/mocha.opts",
    "mochaExplorer.env": { "NYC_ROOT_ID": "true" },
    "git.ignoreLimitWarning": true,
    "eslint.autoFixOnSave": true,
    "javascript.suggestionActions.enabled": false
}