section.vbox
  section.w-f-md
    section.hbox.stretch
      aside.aside-lg.bg-light.lter.b-r
        section.vbox
          section.scrollable
            .wrapper
              .text-center.m-b.m-t
                a.thumb-lg(href='#')
                  img(src='{{info.artist.image}}')
                div
                  .h3.m-t-xs.m-b-xs {{info.artist.name}}
                    small.text-muted {{info.artist.disambiguation}}
               
                  
              .panel.wrapper
                .row.text-center
                  .col-xs-6
                    a(href='#')
                      span.m-b-xs.h4.block {{info.artist.track_count}} 
                      small.text-muted {{info.tracks.length > 1 ? 'Tracks' : 'Track'}}
                  .col-xs-6
                    a(href='#')
                      span.m-b-xs.h4.block {{info.albums.length}} 
                      small.text-muted {{info.albums.length > 1 ? 'Album' : 'Albums'}}
              starratinglabel(artist="info.artist")
                    
              .btn-group.btn-group-justified.m-b
                a.btn.btn-success.btn-rounded(ng-click="starArtist()")
                  span.text
                    i.alloy-icon.icon(ng-class="info.artist.starred === 'true' ? 'icon-star' : 'icon-star-o'")
                    |  {{info.artist.starred === 'true' ? 'Starred' : 'Add to Starred'}}
       
                a.btn.btn-dark.btn-rounded(ng-click="shuffle()")
                  i.alloy-icon.icon-shuffle.icon
                  |  Shuffle
              div
                small.text-uc.text-xs.text-muted Path
                p {{info.artist.path}}
                small.text-uc.text-xs.text-muted Size
                p {{info.size}}
                small.text-uc.text-xs.text-muted Type
                p {{info.artist.type}}
                small.text-uc.text-xs.text-muted Status
                p {{info.artist.status}}
                small.text-uc.text-xs.text-muted info
                p {{info.artist.biography}}
                .line
                small.text-uc.text-xs.text-muted 
                p.m-t-sm 
                  a.btn.btn-rounded.btn-twitter.btn-icon(href='{{link.target}}', target="_blank", ng-repeat="link in info.links")
                    i.alloy-icon.icon(ng-class="$parent.getLinkIcon(link)")

      aside.bg-white
        section.vbox
          header.header.bg-light.lt
            ul.nav.nav-tabs.nav-white
              li.active
                a(data-target='#activity', data-toggle='tab') Albums
              li
                a(data-target='#events', data-toggle='tab') Tracks
              li
                a(data-target='#interaction', data-toggle='tab') Other
          section.scrollable
            .tab-content
              #activity.tab-pane.active
                albumslist(data="info.albums", showArtist='false')
               
              #events.tab-pane
                tracklist(data='info.tracks', showstar='true', showtracknum='true', showartist='false', showalbum='true', showgenre='true', showplays='true', showduration='true')
              #interaction.tab-pane
                .text-center.wrapper
                  i.fa.fa-spinner.fa.fa-spin.fa.fa-large
      



