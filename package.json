    "stats-webpack-plugin": "^0.7.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.19.1",
    "uglify-loader": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-livereload-plugin": "^1.2.0"
  }
}
