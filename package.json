{
  "name": "alloy",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "cross-env MODE=prod electron .",
    "postinstall": "electron-builder install-app-deps & node node_modules/sharp/install/dll-copy",
    "rebuild": "electron-builder install-app-deps & node node_modules/sharp/install/dll-copy",
    "build_styles": "gulp scss:alloydb",
    "build_fonts": "icon-font-generator ./common/icons/*.svg -n alloyicons --normalize --html false -j false -o alloydbweb/styles/fonts",
    "build_web": "cd alloydbweb && cross-env MODE=prod webpack .",
    "build": "npm run build_styles & npm run build_fonts & npm run build_web & npm run dist",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "install": "npm install && cd alloydbweb && npm install"
  },
  "build": {
    "npmRebuild": true,
    "directories": {
      "buildResources": "./common"
    },
    "electronVersion": "4.1.5",
    "electronDownload": {
      "version": "4.1.5"
    },
    "appId": "com.d3bug.alloy",
    "win": {
      "target": "nsis",
      "files": [
        "!node_modules/ffmpeg-static/bin/darwin${/*}",
        "!node_modules/ffmpeg-static/bin/linux${/*}",
        "!node_modules/ffprobe-static/bin/darwin${/*}",
        "!node_modules/ffprobe-static/bin/linux${/*}"
      ]
    },
    "mac": {
      "category": "public.app-category.productivity",
      "files": [
        "!node_modules/ffmpeg-static/bin/win${/*}",
        "!node_modules/ffmpeg-static/bin/linux${/*}",
        "!node_modules/ffprobe-static/bin/win${/*}",
        "!node_modules/ffprobe-static/bin/linux${/*}"
      ]
    },
    "files": [
      "config.js",
      "main.js",
      "package.json",
      "migrations/*.js",
      "css/**/*",
      "alloydbapi/**/*",
      "alloydbui/**/*",
      "alloydbweb/**/*",
      "common/**/*",
      "js/**/*",
      "html/**/*",
      "img/**/*",
      {
        "from": "./",
        "to": "./",
        "filter": [
          "**/*.env"
        ]
      },
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json",
      "node_modules/ffprobe-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffprobe-static/index.js",
      "node_modules/ffprobe-static/package.json"
    ]
  },
  "author": "Jesse Nelson",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "angular": "^1.6.6",
    "better-queue": "^3.8.10",
    "better-sqlite3": "^5.4.0",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.4.1",
    "bootstrap-material-design": "^4.1.1",
    "bootstrap-select": "^1.13.10",
    "cli-source-preview": "^1.1.0",
    "cron": "^1.7.1",
    "crypto-js": "^3.1.9-1",
    "dart-sass": "^1.20.1",
    "db-migrate": "^0.11.5",
    "db-migrate-sqlite3": "^0.4.0",
    "debug": "^4.1.1",
    "del": "^4.1.1",
    "dotenv": "^8.0.0",
    "drivelist": "^8.0.6",
    "electron-log": "^3.0.5",
    "electron-sharp": "^0.20.8",
    "electron-stream": "^7.0.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.4",
    "express-swagger-generator": "^1.1.14",
    "ffmpeg-static": "^2.5.0",
    "ffprobe-static": "^3.0.0",
    "file-stream-rotator": "^0.4.1",
    "file-type": "^11.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^8.0.1",
    "jade": "^1.11.0",
    "jquery": "^3.4.1",
    "js-md5": "^0.7.3",
    "klaw-sync": "^6.0.0",
    "livereload": "^0.7.0",
    "lodash": "^4.17.11",
    "map": "^1.0.1",
    "moment": "^2.24.0",
    "nat-upnp": "^1.1.1",
    "node-watch": "^0.6.2",
    "object-hash": "^1.3.1",
    "popper.js": "^1.15.0",
    "screenres": "^2.0.1",
    "semver": "^6.2.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.22.0",
    "shelljs": "^0.8.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "sqlite3": "^4.0.8",
    "strtok3": "^2.3.0",
    "three": "^0.83.0",
    "triple-beam": "^1.3.0",
    "underscore": "^1.9.1",
    "winston": "^3.2.1",
    "winston-compat": "^0.1.4",
