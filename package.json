    "babel-plugin-angularjs-annotate": "^0.9.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "electron": "^4.1.5",
    "electron-builder": "^20.40.2",
    "exports-loader": "^0.7.0",
